            <text>Summary of Applicable Law</text>
            <type display_value="Line Separator">a93fc8e00f530000b12e6903cfe012e6</type>
        </sys_ui_annotation>
        <sys_ui_annotation action="INSERT_OR_UPDATE">
            <is_plain_text>false</is_plain_text>
            <sys_created_by>john.suits</sys_created_by>
            <sys_created_on>2018-01-26 19:10:59</sys_created_on>
